<apex:page sidebar="true">
  
  <apex:form >
   <apex:pageBlock title="Employee Login Page">
   <apex:pageBlockSection >
   <apex:pageBlockSection columns="1">
   <apex:image value="{!$Resource.emp}"/>

   </apex:pageBlockSection>

   <apex:pageBlockSection title="Login Here" columns="1" collapsible="false">
   
   <apex:pageBlockSectionItem >
   <apex:outputLabel >User id</apex:outputLabel>
   <apex:inputText />   
   </apex:pageBlockSectionItem>
   <apex:pageBlockSectionItem >
   <apex:outputLabel >Password</apex:outputLabel>
   <apex:inputText />   
   </apex:pageBlockSectionItem>
  
   <apex:panelGrid columns="3" style="margin-left:100px" >
   <apex:commandButton value="Login"/>
   <apex:commandButton value="Cancel"/>
   <apex:commandButton value="Register"/>
   </apex:panelGrid>
     </apex:pageBlockSection>
    </apex:pageBlockSection>
   </apex:pageBlock>
  
  </apex:form>
</apex:page>